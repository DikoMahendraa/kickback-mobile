{
  "expo": {
    "name": "kickback-mobile",
    "slug": "kickback-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo/logo-amanahfy.png",
    "scheme": "mobileappamanahfy",
    "newArchEnabled": true,
    "userInterfaceStyle": "light",
    "statusBar": {
      "style": "dark",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "userInterfaceStyle": "light",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobileappamanahfy",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "UIStatusBarStyleDefault",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "userInterfaceStyle": "light",
      "statusBar": {
        "style": "dark",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.mobileappamanahfy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd762324-7201-4dc8-9b94-97e0c83f6c1c"
      }
    },
    "owner": "dikomahendr4",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bd762324-7201-4dc8-9b94-97e0c83f6c1c"
    }
  }
}
